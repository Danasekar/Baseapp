<h2> WEB NOTE</h2>
<body class = "post_new">

<%= form_for @post do |f| %>
<p>
  <%= f.label  :Title %><br>
  <%= f.text_field :title %>
</p>

<p>
  <%= f.label  :"Content" %><br>

  <%= f.text_area :content ,:class => "textarea" %> 
</p>

<p>
   <%= f.submit :Post, :class => "button" %>
</p>

<% end %>
</body>	